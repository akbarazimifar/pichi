{
  "type": "http",
  "bind": [
    {
      "host": "::1",
      "port": 3128
    },
    {
      "host": "127.0.0.1",
      "port": 3128
    }
  ],
  "credentials": [
    {
      "username": "http_proxy_user_000",
      "password": "http_proxy_pass_000"
    },
    {
      "username": "http_proxy_user_001",
      "password": "http_proxy_pass_001"
    }
  ],
  "tls": {
    "key_file": "/etc/letsencrypt/live/example.com/privkey.pem",
    "cert_file": "/etc/letsencrypt/live/example.com/fullchain.pem"
  }
}